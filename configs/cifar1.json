{
    "network": "VGG11",
    "dataset": "cifar10",
    "batch_size": 128,
    "learning_rate": 0.1,
    "num_classes": 10,
    "use_flex": false,
    "optimizer": "SGD",
    "logits_mechanism": "THRESHOLD",
    "masking_mechanism": "SIGMOID_HARD",
    "logits_use_batchnorm": true,
    "dropout": 0.5,
    "in_dimensions": [
        3,
        32,
        32
    ],
    "epochs": 120,
    "spiking": false,
    "spiking_threshold": 1.0,
    "spiking_decay": 0.95,
    "use_stbp": false
  }